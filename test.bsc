// Iterative

digit sum 
sum = 0

cycle (digit i = 1: i <= 5: i++){
	sum = sum + i
}

show("The total sum is: " , sum)

// Assignment

digit x = 10
digit y = 5
digit sum = x + y
indiv letter = 'A'

show("Value of x: ", x)
show("Value of y: ", y)
show("Sum is: ", sum)
show("Current letter: ", letter)

